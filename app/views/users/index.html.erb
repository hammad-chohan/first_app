<h1 align="center">All Alpha Blogger's</h1>
<%= will_paginate %>
<% @users.each do |user| %>
  <ul class="listing">
    <div class="row">
      <div>
      <li><%= link_to gravatar_for(user), user_path(user) %></li>
        <li><%= link_to user.username, user_path(user) %></li>
        <li><small><%= pluralize(user.articles.count, "article") if user.articles %></small></li>
      </div>
    </div>
  </ul>
<% end %>
<%= will_paginate %>